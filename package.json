{
  "name": "react-testing-2",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "styled-components": "^5.1.1"
  },
  "scripts": {
    "test": "react-scripts test",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint --ignore-path .gitignore .",
    "test:coverage": "react-scripts test --coverage",
    "isready": "yarn format && yarn lint && yarn test:coverage",
    "test:staged": "cross-env CI=true react-scripts test --env=jsdom --findRelatedTests",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|tsx|json|css|html|md)\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "eslintIgnore": [
    "node_modules",
    "coverage"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "proseWrap": "always",
    "jsxSingleQuote": false,
    "jsxBracketSameLine": false,
    "quoteProps": "as-needed",
    "htmlWhitespaceSensitivity": "css"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.+(js|ts|graphql|yml|yaml|vue|tsx)": [
      "eslint --fix",
      "prettier --write",
      "yarn test:staged"
    ],
    "**/*.+(css|sass|less|scss|json|html)": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "eslint-config-prettier": "^6.11.0",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "is-ci-cli": "^2.1.2",
    "jest-axe": "^3.4.0",
    "jest-styled-components": "^7.0.2",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "./src/*/*.types.{js,jsx,ts,tsx}",
      "./src/index.{js,tsx}",
      "./src/serviceWorker.{js,ts}"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 30,
        "branches": 30,
        "functions": 30,
        "lines": 30
      }
    },
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "snapshotSerializers": [
      "jest-styled-components"
    ]
  }
}
